        "\n",
        "# Function to calculate ROUGE scores\n",
        "def calculate_rouge(human, generated):\n",
        "    scores = scorer.score(human, generated)\n",
        "    return {\n",
        "        \"ROUGE-1\": scores[\"rouge1\"].fmeasure,\n",
        "        \"ROUGE-2\": scores[\"rouge2\"].fmeasure,\n",
        "        \"ROUGE-L\": scores[\"rougeL\"].fmeasure\n",
        "    }\n",
        "\n",
        "# Calculate ROUGE scores for each model's summary\n",
        "pegasus_rouge = calculate_rouge(human_summary, pegasus_summary)\n",
        "bart_rouge = calculate_rouge(human_summary, bart_summary)\n",
        "t5_rouge = calculate_rouge(human_summary, t5_summary)\n",
        "\n",
